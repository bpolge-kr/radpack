{
  "name": "@radpack/rollup-plugin",
  "version": "1.0.0-rc",
  "description": "Rapid Application Development Packaging Rollup Plugin",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js"
    }
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/radpack.git",
    "directory": "packages/rollup-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@radpack/core": "^1",
    "rollup": "^2"
  },
  "dependencies": {
    "@radpack/build": "1.0.0-rc",
    "acorn-walk": "^8.1.0",
    "magic-string": "^0.25.7"
  },
  "devDependencies": {
    "@radpack/core": "1.0.0-rc",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "acorn": "^8.2.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0"
  },
  "license": "MIT"
}
