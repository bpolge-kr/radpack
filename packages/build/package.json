{
  "name": "@radpack/build",
  "version": "1.0.0-rc",
  "description": "Rapid Application Development Packaging Build Core",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "src/",
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/radpack.git",
    "directory": "packages/build"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@radpack/core": "^1"
  },
  "dependencies": {
    "find-up": "^5.0.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@radpack/core": "1.0.0-rc",
    "rimraf": "^3.0.2"
  },
  "license": "MIT"
}
